{
    "gitbook": ">=3.0.0",
    "title": "Eloquent JavaScript",
    "description": "Eloquent JS - fr",
    "language": "fr",
    "structure": {
        "readme": "README.md"
    },
    "plugins": ["js-console", "github", "offline", "page-numbering"],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/js-appsmaker/eloquentJS-3rd_fr"
        },
        "page-numbering": {
            "skipReadme": false,
            "chapterFormat": "Chapter #chapno#&nbsp;&nbsp;&nbsp;#title# ||| #chapno#&nbsp;&nbsp;&nbsp;#title#",
            "forceMultipleParts": false
        }
    },
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    }
}